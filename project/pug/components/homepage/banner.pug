#homepageBanner
    .overly
    .container-fluid.wow.fadeInUp
        .logo_contact
            a.logo(href=projectUrl+"/") 
                img(src=projectUrl+"/images/logos/logo.png", alt="Voice Logo")
            .contact_container
                .contact.text-center
                    span Contact us at 
                        a(href="tel:+"+contactNumberURL) #{contactNumber} 
                        span or  
                        a.contact_us_link(href=projectUrl+"/contact_us.html") Click Here
            ul.social.list-unstyled 
                each val in socialMediaHomepage 
                    li 
                        a(href=val[1]) 
                            img.img-fluid(src=projectUrl+'/images/social/'+val[0] , alt=val[2] , target="_blank")
        .demo 
            h2 Affordable, Reliable and Flexible: VoIP for Modern Enterprises.  
            a(href=projectUrl+"contact_us.html") Request A Demo